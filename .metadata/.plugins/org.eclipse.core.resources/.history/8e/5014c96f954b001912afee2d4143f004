package com.jlp;

import java.io.BufferedReader;
import java.io.InputStreamReader;

import com.jlp.format.TimeConverter;

public class App {
	public static void main(String[] args) {
		TimeConverter time = new TimeConverter();	
		BufferedReader inputSeonds = new BufferedReader(new InputStreamReader(System.in));
		System.out.println("Please enter the number");
		inputSeonds = Integer.parseInt(inputSeonds.readLine());		
		System.out.println( time.formatTime(86450123));
	}

}
